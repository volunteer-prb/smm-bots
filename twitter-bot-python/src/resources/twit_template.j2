<tr>
    <td>{{ author_name }}</td>
    <td><img src="{{ author_profile_image_url }}"></td>
    <td>{{ created_at }}</td>
    <td>{{ text }}</td>
    <table>
        <tr>
            <td>like: {{ like_count }}</td>
            <td>retweet: {{ retweet_count }}</td>
            <td>reply: {{ reply_count }}</td>
        </tr>
    </table>
</tr>